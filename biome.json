{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": {
		"ignoreUnknown": false,
		"ignore": ["**/routeTree.gen.ts"]
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"jsxQuoteStyle": "single",
			"semicolons": "asNeeded",
			"indentStyle": "space",
			"indentWidth": 2,
			"trailingCommas": "es5",
			"lineWidth": 80,
			"attributePosition": "multiline"
		}
	}
}
